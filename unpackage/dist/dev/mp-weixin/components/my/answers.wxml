<view class="container"><view class="content"><view class="tab-vux" line-width="{{3}}" custom-bar-width="60px" default-color="#C6C6C6" active-color="#de3b12" bar-active-color="#de3b12"><view class="{{['content-item',(tab===1)?'active':'']}}" selected data-event-opts="{{[['tap',[['handleTab',[1]]]]]}}" bindtap="__e">答题活动</view><view data-event-opts="{{[['tap',[['handleTab',[2]]]]]}}" class="{{['content-item',(tab===2)?'active':'']}}" bindtap="__e">题库练习</view></view><view-box class="vue-ref" hidden="{{!(tab==1)}}" vue-id="1" infinite-scroll-disabled="{{loading}}" infinite-scroll-distance="60" data-ref="viewBox" bind:__l="__l" vue-slots="{{['default']}}"><view class="content-inner"><block wx:for="{{list}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view><view data-event-opts="{{[['tap',[['handleDetail',['$0'],[[['list','',index,'id']]]]]]]}}" class="item" bindtap="__e"><view class="item-title">{{item.info_name}}</view><view class="item-bottom"><view><text class="item-score">得分：</text>{{item.score}}</view><view class="item-time">{{item.updated_at}}</view></view></view><view class="bg"></view></view></block></view><van-divider class="{{[this.list.length==0?'show':'hidden']}}" vue-id="{{('2')+','+('1')}}" contentPosition="center" data-com-type="wx" bind:__l="__l" vue-slots="{{['default']}}">暂无答题记录</van-divider><view class="load-more-view"></view></view-box><view data-ref="viewBox" class="vue-ref" hidden="{{!(tab==2)}}"><view class="content-inner"><block wx:for="{{praList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view><view data-event-opts="{{[['tap',[['handleDetail',['$0'],[[['praList','',index,'id']]]]]]]}}" class="item" bindtap="__e"><view class="item-title">{{item.info_name}}</view><view class="item-bottom"><view><text class="item-score">得分：</text>{{item.score}}</view><view class="item-time">{{item.updated_at}}</view></view></view><view class="bg"></view></view></block></view><van-divider class="{{[this.praList.length==0?'show':'hidden']}}" vue-id="3" contentPosition="center" data-com-type="wx" bind:__l="__l" vue-slots="{{['default']}}">暂无答题记录</van-divider><view class="load-more-view"><load-more vue-id="4" show-loading="{{loading}}" tip="{{load_more_tip}}" bind:__l="__l"></load-more></view></view></view></view>